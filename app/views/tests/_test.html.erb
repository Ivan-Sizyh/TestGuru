<tr>
  <td><%= test.title %></td>
  <td><%= test.questions.count %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.category.title %></td>
  <td><%= test.author.name %></td>
  <td><%= link_to 'Просмотр', test_path(test) %></td>
  <td><%= link_to 'Редактирование', edit_test_path(test) %></td>
  <td><%= link_to 'Удалить',
                  test_path(test),
                  method: :delete,
                  data: { confirm: 'Удалить тест?' } %>
  </td>
</tr>